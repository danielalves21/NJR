
#include <stdio.h>
#include <locale.h>
#include<string.h>

 int main(){
 int vetor[3];
 int i;
 
     printf("Digite os elementos do verbo: \n");
     for(i = 0; i < 3; i++) {
        printf("Digite sua %d nota:",i + 1);
		scanf("%d",&vetor[i]);    	
    
	 }
    // exibindo dados
    printf("\n==EXIBINDO RESULTADOS==\n");
    for (i = 0; i < 3; i++) {
    	printf("nota %d: %d\n",i+1, vetor[1]);
	}
  
 
 return 0;
 
 
}

 

