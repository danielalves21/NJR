#include <stdio.h>
#include <locale.h>
#include<string.h>

 int main(){
 int vetor[5];
 int i;
 
     printf("Digite os elementos do verbo: \n");
     for(i = 0; i < 5; i++) {
        printf("Elementos %d:",i + 1);
		scanf("%d",&vetor[i]);    	
    
	 }
    // exibindo dados
    for (i = 0; i < 5; i++) {
    	printf("Elemento %d: %d\n", i+1, vetor[1]);
	}
  
 
 return 0;
 
 
}

 

