
#include <stdio.h>
#include <locale.h>
#include<string.h>

 int main(){
 
 int numeros[5];
 numeros[0] = 1;
 numeros [1] = 2;
 numeros [2] = 3;
 numeros [3] = 4;
 numeros [4] = 5;
 
 printf("Elementos 1: %d\n",numeros[0]);
 printf("Elementos 2: %d\n",numeros[1]);
 printf("Elementos 3: %d\n",numeros[2]);
 printf("Elementos 4: %d\n",numeros[3]);
 printf("Elementos 5: %d\n",numeros[4]);
 
 
 
 return 0;
 
 
}

 


